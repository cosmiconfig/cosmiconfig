{
  "extends": [
    "eslint:recommended"
  ],
  "env": {
    "node": true
  },
  "rules": {
    "comma-dangle": [2, "always-multiline"],
    "curly": 0,
    "wrap-iife": [2, "inside"],
    "brace-style": 0,
    "comma-style": 2,
    "consistent-this": 0,
    "indent": [2, 2, {
      "SwitchCase": 1
    }],
    "no-console": 0,
    "no-lonely-if": 2,
    "no-nested-ternary": 2,
    "no-use-before-define": [2, "nofunc"],
    "quotes": [2, "single"],
    "semi": 2,
    "space-before-function-paren": [2, "never"],
    "keyword-spacing": [2, { "before": true, "after": true }],
    "space-before-blocks": [2, "always"],
    "space-in-parens": [2, "never"],
    "space-unary-ops": 2,
  }
}
